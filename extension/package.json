{
  "name": "quizifer",
  "displayName": "Quizifer",
  "description": "Learn a language consistenty while using vscode.",
  "version": "1.3.1",
  "publisher": "mukeshsuthar",
  "icon": "docs/icon.png",
  "author": {
    "name": "Mukesh Suthar",
    "email": "mukeshsuthar336@gmail.com"
  },
  "engines": {
    "vscode": "^1.52.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "javascript",
    "quiz",
    "language"
  ],
  "bugs": {
    "url": "https://github.com/SutharMukesh/Quizifer/issues",
    "email": "mukeshsuthar336@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SutharMukesh/Quizifer/tree/main/extension"
  },
  "homepage": "https://github.com/SutharMukesh/Quizifer/blob/main/extension/README.md",
  "activationEvents": [
    "onCommand:quizifer.refreshWebView",
    "onCommand:quizifer.qotd",
    "onStartupFinished",
    "onView:quizifer.sidebar"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "quizifer-sidebar-view",
          "title": "Quizifer",
          "icon": "media/checklist.svg"
        }
      ]
    },
    "views": {
      "quizifer-sidebar-view": [
        {
          "type": "webview",
          "id": "quizifer.sidebar",
          "name": "Quizifer",
          "icon": "media/checklist.svg",
          "contextualTitle": "Quizifer"
        }
      ]
    },
    "commands": [
      {
        "command": "quizifer.qotd",
        "title": "Question of the day (QOTD)",
        "category": "Quizifer"
      },
      {
        "command": "quizifer.refreshWebView",
        "title": "Refresh Debug webview",
        "category": "Quizifer"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "webpack --config ./build/node-extension.webpack.config.js",
    "watch": "concurrently \"rollup -c -w\" \"webpack --watch --config ./build/node-extension.webpack.config.js\"",
    "package": "webpack --mode production --devtool hidden-source-map --config ./build/node-extension.webpack.config.js",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "package-publish": "vsce package --baseImagesUrl https://raw.githubusercontent.com/SutharMukesh/Quizifer/main/extension"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.1.0",
    "@rollup/plugin-typescript": "^8.1.0",
    "@tsconfig/svelte": "^1.0.10",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/polka": "^0.5.2",
    "@types/vscode": "^1.52.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.15.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "rollup": "^2.36.2",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.31.2",
    "svelte-check": "^1.1.28",
    "svelte-preprocess": "^4.6.1",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.2",
    "vscode-test": "^1.4.1",
    "webpack": "^5.15.0",
    "webpack-cli": "^4.3.1"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "polka": "^0.5.2"
  }
}
