{
  "name": "quizifer",
  "displayName": "Quizifer",
  "description": "Learn a language consistenty while using vscode.",
  "version": "1.0.6",
  "publisher": "mukeshsuthar",
  "icon": "docs/icon.png",
  "author": {
    "name": "Mukesh Suthar",
    "email": "mukeshsuthar336@gmail.com"
  },
  "engines": {
    "vscode": "^1.52.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "javascript",
    "quiz",
    "language"
  ],
  "bugs": {
    "url": "https://github.com/SutharMukesh/Quizifer/issues",
    "email": "mukeshsuthar336@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SutharMukesh/Quizifer/tree/main/extension"
  },
  "homepage": "https://github.com/SutharMukesh/Quizifer/blob/main/extension/README.md",
  "activationEvents": [
    "onCommand:quizifer.qotd",
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "quizifer.qotd",
        "title": "Question of the day (QOTD)",
        "category": "Quizifer"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "package": "vsce package --baseImagesUrl https://raw.githubusercontent.com/SutharMukesh/Quizifer/main/extension"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.52.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "eslint": "^7.15.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "typescript": "^4.1.2",
    "vscode-test": "^1.4.1"
  },
  "dependencies": {
    "axios": "^0.21.1"
  }
}